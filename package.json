{
	"name": "D7kj HBuilderX Extension",
	"description": "d7kj自定义插件",
	"displayName": "D7kj HBuilderX Extension",
	"version": "21.1.16.1",
	"publisher": "feifeigd",
	"engines": {
		"HBuilderX": "^3.0.5"
	},
	"categories": [
		"Other"
	],
	"main": "./extension",
	"activationEvents": [
		"onCommand:extension.helloWorld"
	],
	"contributes": {
		"commands": [{
			"command": "extension.helloWorld",
			"title": "Hello World"
		}],
		"menus": {
			"editor/context": [{
					"command": "extension.helloWorld",
					"group": "z_commands",
					"when": "editorTextFocus"
				},
				{
					"group": "z_commands"
				}
			]
		},
		"viewsContainers": {
			"activitybar": [{
				"id": "demoview",
				"title": "Demo View"
			}]
		},
		"views": {
			"demoview": [{
				"id": "extensions.treedemo",
				"name": "Demo.View"
			}]
		}
		
	},
	"extensionDependencies": [
		"plugin-manager"
	],
	"dependencies": {}
}
